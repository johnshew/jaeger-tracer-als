{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "max-line-length": {
            "severity": "warning",
            "options": [
                160,
                {
                    "ignore-pattern": "^import |^export {(.*?)} | //"
                }
            ]
        },
        "no-console": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "object-literal-sort-keys": {
            "severity": "warning",
            "options": [
                "match-declaration-order"
            ]
        },
        "interface-name": [
            true,
            "never-prefix"
        ],
        "ordered-imports": false
    }
}